{
    "name": "emigrant/italy-calculator",
    "description": "Immigration Italy Financial Calculator with B&B Module",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "codeigniter4/framework": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "source": "https://github.com/emigrant/italy-calculator"
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
